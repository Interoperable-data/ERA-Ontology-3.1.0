                                                      )
                                        ] ;
                            rdfs:range skos:Concept ;
                            era:XMLName "CPE_SystemCompatibility" ;
                            era:eratvIndex "4.13.1.8" ;
                            era:inSkosConceptScheme <http://data.europa.eu/949/concepts/etcs-system-compatibilities/ETCSSystemCompatibilities> ;
