                    rdfs:label "Primary Location"@en ;
                    rdfs:seeAlso "http://taf-jsg.info/wp-content/uploads/2024/01/20231018-JGS-Handbook-3.4-with-XSD-3.4.0.0.pdf"^^xsd:anyURI ;
