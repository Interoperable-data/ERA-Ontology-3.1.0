	sh:severity sh:Violation ;
    sh:message "hasBallast (1.1.1.1.4.4): This error is due to the track {?trackLabel} of the section of line with label {?thisLabel}, violating the rule: Y for tracks with permitted speed (parameter 1.1.1.1.2.5) greater than 250km/h."@en ;
    sh:prefixes era:;
	sh:select """
    PREFIX era: <http://data.europa.eu/949/>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
    SELECT DISTINCT $this ?clsLabel
			WHERE {
                $this rdfs:label ?thisLabel .
                $this era:hasPart ?track .
                ?track rdfs:label ?trackLabel .
                ?track era:maximumPermittedSpeed ?mps .
                FILTER (?mps > 250) .			
                FILTER NOT EXISTS {$this era:hasBallast ?value} . 
            }
			""" .

