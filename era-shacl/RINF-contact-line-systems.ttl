    era:scope "local";
	rdfs:comment "Indication of the traction supply system" ;
	era:rinfIndex "1.1.1.2.2.1.2" ;
	sh:path era:energySupplySystem ;
	sh:nodeKind sh:IRI ;
    sh:maxCount 1 ;
