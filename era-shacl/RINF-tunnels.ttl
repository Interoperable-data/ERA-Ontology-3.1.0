era-sh:SectionOfLineShape
	a sh:NodeShape ;
	sh:targetClass era:SectionOfLine.

#
# tunnels
#
era-sh:TunnelShape
	a sh:NodeShape ;
	sh:targetClass era:Tunnel .

era-sh:CommonCharacteristicsSubsetShape
	a sh:NodeShape ;
	sh:targetClass era:CommonCharacteristicsSubset .  # Applies to all Common Characteritics Subset  with tunnels parameters 
	

# tunnelIdentification: # 1.1.1.1.8.2, 1.2.1.0.5.2, 1.2.2.0.5.2
era-sh:TunnelShape sh:property era-sh:TunnelIdentification .
era-sh:TunnelIdentification
    a sh:PropertyShape ;
    era:affectedProperty era:tunnelIdentification;
    era:affectedClass era:Tunnel;
    era:scope "local";  
    rdfs:comment "Unique tunnel identification or unique number within Member State." ;
    era:rinfIndex "1.1.1.1.8.2", "1.2.1.0.5.2", "1.2.2.0.5.2" ;
    sh:path era:tunnelIdentification ;           
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string ;
    sh:severity sh:Violation ;
    sh:message "tunnelIdentification (1.1.1.1.8.2, 1.2.1.0.5.2, 1.2.2.0.5.2): Each Tunnel can be referred with only one identification. This error may be due to having a Tunnel with no identification or with more than one identification."@en .

# lineReferenceTunnelStart - kilometer: 1.1.1.1.8.3
era-sh:TunnelShape sh:property era-sh:LineReferenceTunnelStart .
era-sh:LineReferenceTunnelStart
	a sh:PropertyShape;
    era:affectedProperty era:kilometer;
    era:affectedClass era:Tunnel;
    era:affectedClass era:LinePosition;
    era:scope "local"; 
    sh:minCount 1 ;
    sh:maxCount 1 ;
    era:rinfIndex "1.1.1.1.8.3";
    rdfs:comment "Part of the Start of tunnel that indicates the km of the line at the beginning of a tunnel." ;
	sh:path (era:lineReferenceTunnelStart era:kilometer) ;
    sh:datatype xsd:double;
	sh:severity sh:Violation ;
	sh:message "lineReferenceTunnelStart - kilometer (1.1.1.1.8.3):  The tunnel either does not have a value for kilometer at its beginning, has more than one value, or the value is not a double (real) number."@en .

# lineReferenceTunnelEnd - kilometer: 1.1.1.1.8.4
era-sh:TunnelShape sh:property era-sh:LineReferenceTunnelEnd .
era-sh:LineReferenceTunnelEnd
	a sh:PropertyShape;
    era:affectedProperty era:kilometer;
    era:affectedClass era:Tunnel;
    era:affectedClass era:LineReference;
    era:scope "local"; 
    sh:minCount 1 ;
