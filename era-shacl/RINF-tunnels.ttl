era-sh:LengthT
	a sh:PropertyShape ;
    era:affectedProperty era:lengthOfTunnel;
    era:affectedClass era:Tunnel;
    era:scope "local";
	rdfs:comment "Length of a tunnel in metres from entrance portal to exit portal." ;
