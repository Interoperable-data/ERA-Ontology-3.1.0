# OnlyApplicableIfConstraintComponent
era-sh:OnlyApplicableIfConstraintComponent a sh:ConstraintComponent ;
	sh:parameter [
		sh:path era-sh:ConditionalTypeProperty;
		sh:nodeKind sh:IRI
		] ;
	sh:parameter [
		sh:path era-sh:ConditionalValue ;
		sh:nodeKind sh:IRI;
		sh:optional true
		] ;
	sh:propertyValidator era-sh:OnlyApplicableIfSelectValidator .

era-sh:OnlyApplicableIfSelectValidator a sh:SPARQLSelectValidator ;
	sh:select """
    PREFIX era: <http://data.europa.eu/949/>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    SELECT DISTINCT $this ?clsLabel
			WHERE {
                 $this $conditionalTypeProperty $conditionalValue .
				OPTIONAL{$this rdfs:label ?thisLabel} .
                FILTER NOT EXISTS {$this $PATH ?value} . 
            }
			""" .

